@using WeatherResearcher.Models
@model ForWeatherSnapshotModel
<style>
	@@font-face {
		font-family: 'FontAwesome';
		src: url('https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/fonts/fontawesome-webfont.woff2') format('woff2'), url('https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/fonts/fontawesome-webfont.woff') format('woff'), url('https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/fonts/fontawesome-webfont.ttf') format('truetype');
		font-weight: normal;
		font-style: normal
	}

	.glyphicon {
		display: inline-block;
		font: normal normal normal 14px/1 FontAwesome;
		font-size: inherit;
		text-rendering: auto;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale
	}

	.glyphicon-chevron-right:before {
		content: "\f054";
	}

	.glyphicon-chevron-left:before {
		content: "\f053";
	}
</style>
<!-- Main -->
<section id="main">
	<div class="container">
		<div class="row">
			<div class="col-12">
				<!-- Portfolio -->
				<section>
					<header class="major">
						<h2>Filtration</h2>
					</header>
					<form method="get">
						<header>

							<h3>City: </h3>
							<input type="text" name="filter.City" value="@Model.filter.City" class="form-control" />
							<br />
							<h3>Country: </h3>
							<input type="text" name="filter.Country" value="@Model.filter.Country"
								   class="form-control" />
							<br />
							<h3>Time: </h3>
							<input type="datetime" name="filter.TopWeather.Time" value="@Model.filter.TopWeather.Time" class="form-control" />
							<input type="datetime" name="filter.BottomWeather.Time" value="@Model.filter.BottomWeather.Time" class="form-control" />
							<br />

							<h3>Temp: </h3>
							<input type="number" name="TopWeather.temp" value="@Model.filter.TopWeather.temp"
								   class="form-control" />
							<input type="number" name="BottomWeather.temp" value="@Model.filter.BottomWeather.temp"
								   class="form-control" />
							<br />

							<h3>Temp feels like: </h3>
							<input type="number" step="0.01" name="TopWeather.temp_feels_like" value="@Model.filter.TopWeather.temp_feels_like"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.temp_feels_like" value="@Model.filter.BottomWeather.temp_feels_like"
								   class="form-control" />
							<br />

							<h3>Temp Min: </h3>
							<input type="number" step="0.01" name="TopWeather.temp_min" value="@Model.filter.TopWeather.temp_min"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.temp_min" value="@Model.filter.BottomWeather.temp_min"
								   class="form-control" />
							<br />

							<h3>Temp Max: </h3>
							<input type="number" step="0.01" name="TopWeather.temp_max" value="@Model.filter.TopWeather.temp_max"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.temp_max" value="@Model.filter.BottomWeather.temp_max"
								   class="form-control" />
							<br />

							<h3>Pressure: </h3>
							<input type="number" step="0.01" name="TopWeather.pressure" value="@Model.filter.TopWeather.pressure"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.pressure" value="@Model.filter.BottomWeather.pressure"
								   class="form-control" />
							<br />

							<h3>Humidity: </h3>
							<input type="number" step="0.01" name="TopWeather.humidity" value="@Model.filter.TopWeather.humidity"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.humidity" value="@Model.filter.BottomWeather.humidity"
								   class="form-control" />
							<br />

							<h3>Wind speed: </h3>
							<input type="number" step="0.01" name="TopWeather.wind_speed" value="@Model.filter.TopWeather.wind_speed"
								   class="form-control" />
							<input type="number" step="0.01" name="BottomWeather.wind_speed" value="@Model.filter.BottomWeather.wind_speed"
								   class="form-control" />
							<br />
						</header>
						<footer>
							<ul class="actions">
								<input type="submit" value="Filtrate" class="button alt" />
							</ul>
						</footer>
					</form>
